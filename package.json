{
  "name": "elumian",
  "version": "1.1.9",
  "description": "Elumian is a framework for back end developers to deploy an api quickly and securely.",
  "main": "lib/*",
  "homepage": "https://github.com/KrashMello/elumian",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/KrashMello/elumian"
  },
  "keywords": [
    "elumian",
    "elumian-ts",
    "lib",
    "framework"
  ],
  "types": "lib/type.d.ts",
  "files": [
    "/lib"
  ],
  "author": "krashMello <joelenrikesoteldo@gmail.com> (https://krashmello.netlify.app/)",
  "license": "MIT",
  "scripts": {
    "tsc": "tsc",
    "build": "tsc -b -v src",
    "build:alias": "rm -rf ./lib && rm -f tsconfig.tsbuildinfo && bunx tsc && tsc-alias -p tsconfig.json",
    "build:watch": "tsc -b -v src --watch",
    "start": "node dist/src/index.js",
    "start:example": "ts-node-dev --rs --watch --clear -r tsconfig-paths/register example/index.ts",
    "migrate": "ts-node example/migration/index.ts",
    "dev": "ts-node -r example/index.ts",
    "test": "vitest",
    "lint": "ts-standard",
    "lint:fix": "ts-standard --fix",
    "db:generate": "prisma generate --schema=./example/prisma/scheme.prisma",
    "db:push": "prisma db push --schema=./example/prisma/scheme.prisma",
    "db:seed": "ts-node ./example/prisma/seed.ts"
  },
  "devDependencies": {
    "@types/twig": "^1.12.16",
    "module-alias": "^2.2.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "ts-standard": "^12.0.2",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "4.2.0",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@prisma/client": "^5.11.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "4.17.21",
    "@types/lodash": "^4.14.196",
    "@types/node": "^20.12.2",
    "@types/validator": "13.11.9",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "4.19.2",
    "lodash": "^4.17.21",
    "prisma": "^5.11.0",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.7.2",
    "tslib": "^2.6.3",
    "twig": "^1.17.1",
    "typescript": "*",
    "validator": "13.11.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    }
  }
}
